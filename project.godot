config_version=5

[application]

config/name="Flogtus"
config/version="0.0.1 beta"
run/main_scene="uid://bn1m4wpx3mgob"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.4")
run/max_fps=60
boot_splash/bg_color=Color(0.184314, 0.243137, 0.203922, 1)
config/icon="uid://cb83vsev3piy2"
config/windows_native_icon="res://flogtus_icon.ico"

[autoload]

Utility="*res://scripts/global/util.gd"
Data="*res://scripts/global/data.gd"
LeapServer="*res://scripts/global/leap_server.gd"
